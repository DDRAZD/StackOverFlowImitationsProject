@model List<StackOverflowProject.ViewModels.QuestionViewModel>

@{
ViewBag.Title="Home";
}

<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h1>Learn, Share, Build!</h1>
            <h3>Each month, over 50 million developres come to StackOverflow to learn, share knowledge and build their careers</h3>
            <p class="lead">Join the world's leading developer community</p>
            
        </div>

    </div>
   
</div><!--end of first row-->

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach(var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>

        @if(Session["CurrentUserName"] != null)
            {
                <a href="/questions/create" class="btn btn-primary">Add Question</a>
            }
    </div>

</div>

<script>
    $("a[href='/home/inedx']").attr("id", "active");
</script>


