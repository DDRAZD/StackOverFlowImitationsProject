@model StackOverflowProject.ViewModels.QuestionViewModel

<tr>
    <td><span class="badge bg-secondary">@Model.VotesCount</span> Votes</td>
    <td><span class="badge bg-secondary">@Model.AnswersCount</span> Answers</td>
    <td><span class="badge bg-secondary">@Model.ViewsCounter</span> Views</td>
    <td class="text-dark">
        <a href="/question/view?qID=@Model.QuestionID" class="text-primary"> @Model.QuestionName </a>
        <br />
        <span class="badge bg-info">@Model.Category.CategoryName</span>
    </td>
    <td>
        @if (Convert.ToInt32(Session["CurrentUserID"]) != Model.UserID)
        {
            @: by @Model.User.Name
        }
        else
        {
            @: by you
        }

    </td>
    <td> on @Model.QuestionDateAndTime</td>
</tr>